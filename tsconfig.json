{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["dom", "es5", "es6", "scripthost"],
    "sourceMap": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
